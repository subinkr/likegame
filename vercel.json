{
  "version": 2,
  "builds": [
    {
      "src": "build/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/manifest.json",
      "dest": "/build/web/manifest.json",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/build/web/$1"
    }
  ],
  "outputDirectory": "build/web"
}
